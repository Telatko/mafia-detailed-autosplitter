  var cid = -1;
  string scene = null;
  string mid = null;
  List<string[]> checkpoint = null;
  var hasSettingsKey = new HashSet<string>();

  foreach (var split in vars.splitDefinitions) {
    if (split[1] == "mission") {
      mid = "mission-"+split[0];
      settings.Add(mid, true, split[0], "fine-tuned-splits");
    }
    else if(split[1] == "scene") {
      scene = split[0];
    }
    else if(split[1] == "checkpoint") {
      checkpoint = new List<string[]>();
      var splitId = "checkpoint-"+split[3];
      checkpoint.Add(new string[] { split[0], split[1], split[2], split[3], split[4], split[5], splitId });
      cid = int.Parse(split[3]);
      vars.checkpoints[cid] = checkpoint;
      if (cid > 0) {
        settings.Add(splitId, split[2].Contains("real"), split[0], mid);
      }
    }
    else {
      var splitId = cid+"|"+split[0]+"|"+split[1];
      foreach(var cat in split[2].Split(' '))
        if (cat.Length > 0 && cat[0] == ':')
          splitId = cat.Substring(1);
      checkpoint.Add(new string[] { split[0], split[1], split[2], split[3], split[4], split[5], splitId });
      if (!hasSettingsKey.Contains(splitId)) {
        settings.Add(splitId, split[2].Contains("real"), "    ["+split[1]+"] "+split[0], mid);
        hasSettingsKey.Add(splitId);
      }
    }
  }
}

update
{
  if (!vars.setFinalCutscene && current.language != "") {
    if (current.language == "cz") vars.finalCutscene = 2;
    else if (current.language == "de") vars.finalCutscene = 3;
    else if (current.language == "sp") vars.finalCutscene = 1;
    else vars.finalCutscene = 0;
    vars.setFinalCutscene = true;
  }

  if (version == "") return;    // If version is unknown, don't do anything (without it, it'd default to "1.0" version)

  if (current.mission != null) {
    vars.lastMission = current.mission;
  }

  if (old.mission == "00menu" && current.mission != "00menu") {
    vars.crash = false;
    timer.IsGameTimePaused = false;
  }

  if (!current.isLoaded) {
    vars.lastCheckpointTime = DateTime.Now;
  }
}

start
{
  if (current.mission == "extrem" && !current.inMission && old.isLoading3 && !current.isLoading3)
    return true;

  if ((!old.m1Cutscene && current.m1Cutscene && current.mission == "mise01") || (settings["checkpoint-start"] && current.checkpoint > 0 && ((old.isLoading3 && !current.isLoading3) || (current.isLoading3 && Array.IndexOf(vars.fastCheckpoints, current.checkpoint) > -1)))) {
    vars.mandatoryCheckpoints = (settings["bertone-required"] ? (new List<int>(vars.mainCheckpoints)).Concat(new List<int>(vars.bertoneCheckpoints)).ToArray() : vars.mainCheckpoints);
    Array.Sort(vars.mandatoryCheckpoints);
    vars.lastCheckpoint = current.checkpoint;
    vars.nextMandatoryCheckpointIndex = 1;
    while (vars.mandatoryCheckpoints[vars.nextMandatoryCheckpointIndex] <= current.checkpoint)
      vars.nextMandatoryCheckpointIndex++;
    vars.splitDone.Clear();
    vars.nextCutscene = 0;
    vars.debug = new List<string>();
    vars.splits = vars.checkpoints[current.checkpoint];
    vars.carEnteredAt = DateTime.Now;
    vars.currentTransition = null;
    vars.sceneTransitions = new List<string[]>() { new string[] { "60", "mise05-saliery", "70" }, new string[] { "230", "mise10-letiste", "240" }, new string[] { "425", "mise16-mesto", "435" }, new string[] { "495", "mise18-pristav", "500" } };
    string scene = null;
    var checkpoints = new List<string[]>();

    foreach (var split in vars.splitDefinitions) {
      if(split[1] == "scene" && checkpoints.Count > 0)
        scene = split[0];
      if(split[1] != "checkpoint")
        continue;
      if (scene != null) {
        var lastCheckpoint = checkpoints[checkpoints.Count-1];
        if (lastCheckpoint[0] == "Lucas Bertone 2" && !settings["bertone-required"]) {
          vars.sceneTransitions.Add(new string[] { checkpoints[checkpoints.Count-3][3], scene, split[3] });
          vars.sceneTransitions.Add(new string[] { checkpoints[checkpoints.Count-2][3], scene, split[3] });
        }
        vars.sceneTransitions.Add(new string[] { lastCheckpoint[3], scene, split[3] });
        scene = null;
      }
      checkpoints.Add(split);
    }
    vars.routeSteps = -1;
    vars.routeEndEvent = "";
    vars.splitsDue = 0;
    vars.activeRoute = "";

    if (settings["debug"]) {
      vars.startedAt = DateTime.Now;
      vars.logFilePath = Path.Combine(vars.logDirPath, string.Format("{0:yyyy-MM-dd-HH-mm-ss}_"+Process.GetCurrentProcess().Id+".txt", vars.startedAt));
      System.IO.Directory.CreateDirectory(vars.logDirPath);
      print("Log file: "+vars.logFilePath);
    }
    return true;
  }
}

// Reset timer on "An Offer You Can't Refuse" load (you can comment this section out if you don't want this feature)
reset
{
  return (current.mission == "mise01" && ((old.isLoading1 && !current.isLoading1) || (!old.isLoading3 && current.isLoading3)));
}

// Split for every mission change (at the very beginning of every loading)
split
{
  if (current.mission == null || (vars.nextMandatoryCheckpointIndex == -1 && current.mission != "extrem")) return;  // gets rid of null reference expections in debugview

  if (vars.splitsDue > 0) {
    vars.splitsDue--;
    return true;
  }

  if (current.mission.Contains("mise") && old.mission != "00menu") {
    // Final split
    if (old.checkpoint == vars.finalcheckpoint && current.checkpoint == vars.finalcheckpoint) {
      return (old.finalCutscene <= vars.finalCutscene && current.finalCutscene > vars.finalCutscene);
    }

    else if (settings["fine-tuned-splits"] && current.mission != "extrem") {
      var secondsSinceLastCheckpoint = (DateTime.Now-vars.lastCheckpointTime).TotalSeconds;
      var time = DateTime.Now;

      if(vars.debug.Count > 0 && settings["debug"]) {
        var duration = TimeSpan.FromSeconds((time-vars.startedAt).TotalSeconds).ToString(@"hh\:mm\:ss\.ff");
        using (var file = new StreamWriter(vars.logFilePath, append: true))
          foreach (string debugLine in vars.debug)
            file.WriteLine(duration+" | "+debugLine);
        vars.debug.Clear();
      }

      if(old.language != current.language) vars.debug.Add("  language: "+current.language);
      if(old.item != current.item) vars.debug.Add("  item: "+current.item);
      if(old.cutscene != current.cutscene) vars.debug.Add("  cutscene: "+current.cutscene);
      if(old.scene != current.scene) vars.debug.Add("  scene: "+current.scene);
      if(old.checkpoint != current.checkpoint) vars.debug.Add("  checkpoint: "+current.checkpoint);
      if(old.sound1 != current.sound1) vars.debug.Add("  sound1: "+current.sound1);
      if(old.sound2 != current.sound2) vars.debug.Add("  sound2: "+current.sound2);
      if(old.objectiveId != current.objectiveId) vars.debug.Add("  objectiveId: "+current.objectiveId);

      if (current.checkpoint == vars.lastCheckpoint && old.scene != current.scene) {
        foreach(var tran in vars.sceneTransitions) {
          if (tran[0] == current.checkpoint+"" && tran[1] == current.scene && settings["checkpoint-"+tran[2]]) {
            vars.currentTransition = tran;
            vars.debug.Add("TRANSITION SPLIT "+current.checkpoint);
            if (vars.activeRoute != "") {
              vars.splitsDue += vars.remainingSteps;
              vars.activeRoute = "";
            }
            return true;
          }
        }
      }
      if (old.cutscene != 6620520 && current.cutscene == 6620520) {
        vars.carEnteredAt = DateTime.Now;
      }
      
      if (secondsSinceLastCheckpoint < 1) {
        vars.nextCutscene = 0;
      }

      if (vars.lastCheckpoint < current.checkpoint && current.checkpoint <= vars.mandatoryCheckpoints[vars.nextMandatoryCheckpointIndex]) {
        if (Array.IndexOf(vars.bertoneCheckpoints, current.checkpoint) == -1 || settings["bertone-required"])
          foreach(var s in vars.checkpoints[vars.lastCheckpoint])
            if (s[1] != "checkpoint" && settings[s[6]] && !vars.splitDone.Contains(s[6]))
              vars.splitsDue += (s[1] == "route" ? int.Parse(s[3].Split(' ')[5])-1 : 1);
        vars.splits = vars.checkpoints[current.checkpoint];
        vars.lastCheckpoint = current.checkpoint;
        vars.nextCutscene = 0;
        vars.lastCheckpointTime = DateTime.Now;
        vars.debug.Clear();
        if (current.checkpoint == vars.mandatoryCheckpoints[vars.nextMandatoryCheckpointIndex]) {
          vars.nextMandatoryCheckpointIndex++;
        }

        if (vars.activeRoute != "") {
          vars.splitsDue += vars.remainingSteps;
          vars.activeRoute = "";
        }

        foreach(var split in vars.splits) {
          if (split[1] == "checkpoint" && !settings["checkpoint-"+split[3]]) {
            vars.debug.Add("CHECKPOINT SPLIT "+current.checkpoint+" disabled");
            return false;
          }
        }
        var shouldSplit = (vars.currentTransition == null);
        vars.currentTransition = null;
        vars.debug.Add("CHECKPOINT SPLIT "+current.checkpoint+" "+shouldSplit);
        return shouldSplit;
      }

      var splits = (current.checkpoint == vars.lastCheckpoint ? vars.splits : new List<string[]>());
      var cutsceneEnded = false;

      foreach(var split in splits) {
        if (split[4] != "") {
          var skip = true;
          foreach(var req in split[4].Split('#'))
            if (vars.splitDone.Contains(req))
              skip = false;
          if (skip)
            continue;
        }
        if (split[1] == "checkpoint" || (split[5] != "" && split[1] != "route" && vars.splitDone.Contains(split[5])))
          continue;
        var id = split[6];
        var values = split[3].Split(' ');
        current.lap = current.raceLap.Split('/')[0];

        if (split[1] == "route" && settings[id] && (!vars.splitDone.Contains(id) || vars.activeRoute == id)) {
          if(!vars.splitDone.Contains(id)) {
            vars.splitDone.Add(id);
            vars.activeRoute = id;
            vars.routeSteps = int.Parse(values[5]);
            vars.routeEndEvent = split[5];
            vars.remainingSteps = vars.routeSteps - 1;
            vars.debug.Add("  setting up route");
          }

          float x = 0;
          float y = 0;
          if (values[0] == "1") { x = current.x1; y = current.y1; }
          if (values[0] == "2") { x = current.x2; y = current.y2; }

          var fromDx = int.Parse(values[1]) - x;
          var fromDy = int.Parse(values[2]) - y;
          var toDx = int.Parse(values[3]) - x;
          var toDy = int.Parse(values[4]) - y;

          var from = Math.Sqrt(fromDx*fromDx + fromDy*fromDy);
          var to = Math.Sqrt(toDx*toDx + toDy*toDy);
          var progress = vars.routeSteps * (from / (from + to));

          if (progress > int.Parse(values[5]) - vars.remainingSteps && secondsSinceLastCheckpoint > 1) {
            vars.splitsDue++;
            vars.remainingSteps--;
            vars.debug.Add("  progress "+progress+" "+id);
          }
        }

        else if ((!vars.splitDone.Contains(id) || split[1] == "cutscene end") && secondsSinceLastCheckpoint > 2 && (
          (split[1] == "location" && (values[0] == "*" || current.x1 > float.Parse(values[0])) && (values[1] == "*" || current.x1 < float.Parse(values[1])) && (values[3] == "*" || current.y1 < float.Parse(values[3])) && (values[4] == "*" || current.y1 > float.Parse(values[4]))) ||
          (split[1] == "cutscene end" && !cutsceneEnded && (values[0] == "*" || int.Parse(values[0]) == vars.nextCutscene) && old.cutscene == int.Parse(values[1]) && current.cutscene != int.Parse(values[1])) ||
          (split[1] == "objective" && old.objectiveId != current.objectiveId && Array.IndexOf(values, current.objectiveId+"") > -1) ||
          (split[1] == "cutscene start" && !cutsceneEnded && (values[0] == "*" || int.Parse(values[0]) == vars.nextCutscene) && old.cutscene != int.Parse(values[1]) && current.cutscene == int.Parse(values[1])) ||
          (split[1] == "race lap" && current.lap[current.lap.Length-1] == split[3][0] && old.lap != current.lap) ||
          (split[1] == "item" && ((Array.IndexOf(values, old.language+old.item) == -1 && Array.IndexOf(values, current.language+current.item) > -1) || (Array.IndexOf(values, "**"+old.item) == -1 && Array.IndexOf(values, "**"+current.item) > -1))) ||
          (split[1] == "timeout" && secondsSinceLastCheckpoint > int.Parse(split[3])) ||
          (split[1] == "sound" && (Array.IndexOf(values, current.language+current.sound1) > -1 || Array.IndexOf(values, current.language+current.sound2) > -1 || Array.IndexOf(values, "**"+current.sound1) > -1 || Array.IndexOf(values, "**"+current.sound2) > -1)) ||
          (split[1] == "car" && (DateTime.Now-vars.carEnteredAt).TotalSeconds < 5 && Array.IndexOf(values, current.item+"") > -1) ||
          (split[1] == "timer" && current.timeLeft < old.timeLeft)
        )) {
          if (split[1] == "cutscene end") {
            vars.nextCutscene++;
            if (!vars.splitDone.Contains(id)) {
              cutsceneEnded = true;
            }
          }
          if (vars.activeRoute != "" && id == vars.routeEndEvent) {
            vars.splitsDue += vars.remainingSteps;
            vars.activeRoute = "";
          }
          vars.debug.Add("SPLIT ["+id+"] "+settings[id]);
          vars.splitsDue += (settings[id] && !vars.splitDone.Contains(id) ? 1 : 0);
          vars.splitDone.Add(id);
        }
      }

      if (vars.splitsDue > 0) {
        vars.splitsDue--;
        return true;
      }
      return false;
    }

    // Don't split on these mission changes
    else if (current.mission == "mise01") return false;

    // Split during Fairplay
    else if (current.mission == "mise06") {
      return (old.mission == "mise05" && settings["fairplay"]);
    }

    // Split after Sarah
    else if (current.scene == "mise07b-saliery") {
      return (old.scene == "mise07-sara" && settings["sarah"]);
    }

    // Split after The Whore
    else if (current.scene == "mise08-kostel") {
      return (old.scene == "mise08-hotel" && settings["whore"]);
    }

    // Split for any other mission
    return (old.mission != current.mission);
  }
  else if (old.scene == "mise20-galery" && current.scene == "FMV KONEC") {
    return true;
  }
  else {
    return (current.mission == "extrem" && old.inMission && !current.inMission && !current.isLoading3 && !current.isLoading1 && current.isLoaded); // split in case of FRE else false
  }
}

// Load remover  (you can comment this section out if you don't want this feature)
isLoading
{
  if (!vars.crash) {
    // FRE is real time only

    if (current.mission == "extrem") {
      return false;
    }
    else if (current.mission == "00menu") {
      if (old.mission == "extrem") vars.fromExtrem = true;

      return (current.isLoading1 && !vars.fromExtrem);
    }
    else {
      vars.fromExtrem = false;
      return (current.isLoading1 || !current.isLoaded || current.isLoading3);
    }
  }
}

exit
{
  if (vars.lastMission != "extrem") {
    timer.IsGameTimePaused = true;
  }
  vars.crash = true;
}
