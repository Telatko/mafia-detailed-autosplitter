      cid = int.Parse(split[3]);
      vars.checkpoints[cid] = checkpoint;
      if (cid > 0) {
        settings.Add(splitId, split[2].Contains("real"), split[0], mid);
      }
    }
    else {
      var splitId = cid+"|"+split[0]+"|"+split[1];
      foreach(var cat in split[2].Split(' '))
        if (cat.Length > 0 && cat[0] == ':')
          splitId = cat.Substring(1);
      checkpoint.Add(new string[] { split[0], split[1], split[2], split[3], split[4], split[5], splitId });
      if (!hasSettingsKey.Contains(splitId)) {
